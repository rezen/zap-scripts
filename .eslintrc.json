{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "println": "writable",
        "org": "readonly",
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "Java":"writable"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-empty": 0,
        "no-redeclare": 0,
        "no-empty-function": 0,
        "no-mixed-spaces-and-tabs": 0,
        "no-unused-vars": ["error", { 
            "args": "none",
            "varsIgnorePattern": "scan|pscan|appliesToHistoryType|logger" 
        }]
    }
}